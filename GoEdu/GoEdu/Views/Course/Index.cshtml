 @model List<Course>

<h2>جميع الكورسات</h2>

<table class="table">
    <thead>
        <tr>
            <th>اسم الكورس</th>

            <th>السعر</th>
            <th>عدد الساعات</th>
            <th>المستوى</th>
            <th>الفصل الدراسي</th>
            <th>المرحلة الدراسية</th>
            <th>المدرب</th>
            <th>الإجراءات</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Name</td>

                <td>@course.Price</td>
                <td>@course.Hours</td>
                <td>@course.CourseLevel</td>
                <td>@course.Semester</td>
                <td>@course.StudentStage</td>
                <td>
                    @course.Instructor.Name
                <td>

                    <a href="/Course/Details">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>